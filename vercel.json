{
    "version": 2,
    "builds": [
      {
        "src": "api.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/chat",
        "dest": "api.py"
      },
      {
        "src": "/(.*)",
        "dest": "public/index.html"
      }
    ]
  }